{
  "name": "kahlillechelt.com",
  "version": "1.0.0",
  "description": "Kahlil's blog.",
  "scripts": {
    "css": "postcss css/styles.css -o static/css/styles.min.css",
    "css:prod": "postcss --env production --no-map css/styles.css -o static/css/styles.min.css",
    "watch:css": "postcss css/styles.css -o static/css/styles.min.css -w",
    "serve": "hugo serve",
    "start": "yarn serve & yarn watch:css",
    "start:drafts": "hugo serve -D & yarn watch:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kahlil/kahlillechelt.com.git"
  },
  "author": "Kahlil Lechelt",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/kahlil/kahlillechelt.com/issues"
  },
  "homepage": "https://www.kahlillechelt.com",
  "dependencies": {
    "autoprefixer": "10.4.2",
    "browserslist": "4.19.1",
    "cssnano": "5.0.16",
    "postcss": "8.4.6",
    "postcss-cli": "9.1.0",
    "postcss-import": "14.0.2",
    "tailwindcss": "3.0.18"
  },
  "volta": {
    "node": "16.13.2",
    "yarn": "1.22.17"
  }
}
